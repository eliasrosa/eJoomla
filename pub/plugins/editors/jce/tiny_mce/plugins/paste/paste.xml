<?xml version="1.0" ?>
<install version="1.5.0" type="plugin" plugin="paste">
	<name>Paste</name>
	<version>1.5.3</version>
	<creationDate>02 June 2009</creationDate>
	<author>Ryan Demmer</author>
	<authorEmail>ryandemmer@gmail.com</authorEmail>
	<authorUrl>http://www.joomlacontenteditor.net/</authorUrl>
	<copyright>Ryan Demmer</copyright>
	<license>LGPL</license>
	<description><![CDATA[ 
	<p>Paste plugin for JCE 1.5.x</p>	
	]]></description>
	<icon>pastetext,pasteword</icon>
	<layout>paste</layout>
	<params group="standard">
        <param name="paste_keep_linebreaks" type="list" default="1" label="PASTE PARAM LINEBREAKS" description="PASTE PARAM LINEBREAKS DESC">
            <option value="1">YES</option>
			<option value="0">NO</option>
        </param>
		<param name="paste_auto_cleanup_on_paste" type="list" default="1" label="PASTE PARAM CLEANUP" description="PASTE PARAM CLEANUP DESC">
            <option value="0">NO</option>
            <option value="1">YES</option>
        </param>
        <param name="paste_strip_class_attributes" type="list" default="all" label="PASTE PARAM CLASSES" description="PASTE PARAM CLASSES">
            <option value="all">PASTE PARAM CLASSES ALL</option>
            <option value="none">PASTE PARAM CLASSES NONE</option>
            <option value="mso">PASTE PARAM CLASSES MSO</option>
        </param>
        <param name="paste_remove_spans" type="list" default="1" label="PASTE PARAM SPANS" description="PASTE PARAM SPANS DESC">
            <option value="0">NO</option>
            <option value="1">YES</option>
        </param>
		<param name="paste_retain_style_properties" type="text" default="" label="PASTE PARAM RETAIN STYLES" description="PASTE PARAM RETAIN STYLES" />
        <param name="paste_remove_styles" type="list" default="1" label="PASTE PARAM STYLES" description="PASTE PARAM STYLES DESC">
            <option value="0">NO</option>
            <option value="1">YES</option>
        </param>
    </params>
	<languages>
		<language tag="en-GB">en-GB.com_jce_paste.ini</language>
	</languages>
</install>
